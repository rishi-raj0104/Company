{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  